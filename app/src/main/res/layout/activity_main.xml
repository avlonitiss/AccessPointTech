<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    tools:ignore="ExtraText">

    <TextView
        android:id="@+id/incidentCustCompanyText"
        android:layout_width="204dp"
        android:layout_height="44dp"

        android:text=" "


        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.078" />

    <TextView
        android:id="@+id/incidentCustNameText"
        android:layout_width="289dp"

        android:layout_height="43dp"
        android:text=" "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.21" />

    <TextView
        android:id="@+id/incidentDescriptionText"
        android:layout_width="281dp"
        android:layout_height="117dp"
        android:text="  "
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="187dp" />

    <TextView
        android:id="@+id/incidentCustAdText"
        android:layout_width="210dp"
        android:layout_height="34dp"
        android:text="  "
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="331dp" />

    <Button
        android:id="@+id/mapButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Find in map"
        tools:layout_editor_absoluteX="265dp"
        tools:layout_editor_absoluteY="389dp" />

    <CheckBox
        android:id="@+id/finishCheckBox"
        android:layout_width="153dp"
        android:layout_height="49dp"
        android:text="Υπάρχουν εκκρεμότητες"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="388dp" />

    <Button
        android:id="@+id/qrButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="QR-SCAN"
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="480dp" />

    <Button
        android:id="@+id/timeButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="καταγραφη ωρας"
        tools:layout_editor_absoluteX="247dp"
        tools:layout_editor_absoluteY="480dp" />

</androidx.constraintlayout.widget.ConstraintLayout>